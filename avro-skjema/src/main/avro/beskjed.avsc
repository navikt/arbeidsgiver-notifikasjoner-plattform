    },
    {
      "name": "tekst",
      "type": "string",
      "doc": "Teksten som presenteres til brukeren."
    },
    {
      "name": "link",
      "type": "string",
      "doc": "Linken brukeren kan klikke på for å få mer informasjon om bedskjeden"
    }
